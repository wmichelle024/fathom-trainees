{
  "name": "fathom-trainees",
  "version": "1.0.0",
  "description": "Example rulesets that plug into FathomFox's trainer",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "browser": "web-ext run -s addon/",
    "deps": "yarn install --ignore-engines",
    "test": "./node_modules/.bin/mocha --require @babel/register"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-core": "^6.26.3",
    "chai": "^4.2.0",
    "fathom-web": "^3.0.0-alpha.1",
    "mocha": "^6.1.4",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "web-ext": "^1.7.0"
  },
  "start": "nodemon --exec babel-node ./src",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "jsdom": "^15.1.1",
    "mocha-jsdom": "^2.0.0"
  }
}
